// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Vote {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  value      Int
  userId     String
  projectId  String
}

model ProjectModel {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  teamName     String
  projectName  String
  projectLink  String
  members      String[]
  contactEmail String
}
