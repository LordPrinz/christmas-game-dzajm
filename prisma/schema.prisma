// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ProjectModel {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  teamName     String
  projectName  String
  upVoters     String[]
  downVoters   String[]
  projectLink  String
  members      String[]
  contactEmail String
}
